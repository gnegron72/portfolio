{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Technical Product Manager Portfolio - Gabriel Negron",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "start:next": "next start",
    "lint": "eslint .",
    "docker:build": "docker build -t portfolio .",
    "docker:start": "docker run -d -p 3000:3000 --env-file .env.local --name portfolio portfolio",
    "docker:stop": "docker stop portfolio && docker rm portfolio",
    "docker:logs": "docker logs portfolio",
    "docker:restart": "npm run docker:stop && npm run docker:run"
  },
  "keywords": [
    "portfolio",
    "technical-product-manager",
    "next.js",
    "typescript"
  ],
  "author": "Gabriel Negron",
  "license": "ISC",
  "dependencies": {
    "daisyui": "^5.1.7",
    "next": "^15.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2"
  }
}
